
@model EDI.Models.tradingPartnerSetup

<div class="card-body">
    <ul class="nav nav-tabs" role="tablist">
        <li class="nav-item"> <a class="nav-link active" data-toggle="tab" href="#Tamplates" role="tab"><span><i class="ti-user"></i></span> Tamplates </a> </li>
        <li class="nav-item"> <a class="nav-link" data-toggle="tab" href="#Lookups" role="tab"><span><i class="ti-user"></i></span> Lookups</a> </li>
    </ul>
    <div class="tab-content  tabcontent-border">
        <div class="tab-pane active" id="Tamplates" role="tabpanel">
            <div class="p-20">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table full-color-table full-inverse-table hover-table">
                            <thead>
                                <tr>
                                    <th>
                                        <label>  Tamplate Name</label>
                                    </th>
                                    <th>
                                        <label>  Description </label>
                                    </th>
                                    <th>
                                        <label>  Value </label>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => modelItem.id)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelItem => modelItem.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => modelItem.Name)
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        <div class="tab-pane" id="Lookups" role="tabpanel">
            <div class="p-20">
                <div class="row">
                    <label> Lookup Type : </label> @Html.TextBoxFor(x => x.Name);
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table full-color-table full-inverse-table hover-table">
                            <thead>
                                <tr>
                                    <th>
                                        <label>  Conversion Factor </label>
                                    </th>
                                    <th>
                                        <label>  Primary Item # </label>
                                    </th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => modelItem.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => modelItem.Name)
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div>
                    @Html.ActionLink("Export", "Edit", new { id = Model.id }, new { @class = "btn btn-info" }) |
                    @Html.ActionLink("Import", "Edit", new { id = Model.id }, new { @class = "btn btn-info" })
                </div>
            </div>
        </div>
      
    </div>
</div>