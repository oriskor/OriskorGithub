@model IEnumerable<EDI.Models.C850_Header>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MasterPage.cshtml";
}

<h2>Inbox</h2>
<script type="text/javascript">

    $(document).ready(function () {
        $('table tbody tr').db(function (args) {
            debugger;
            var id = $(this).closest("tr").find('td:eq(0)').text().trim();

            url = '@Url.Action("Details", "C850_Header")' + '?id=' + id;
            window.open(
                url,
                '_blank' // <- This is what makes it open in a new window.
            );
        });
    });

</script>
<style>
    .linkone {
        font-size: 12px;
    }
</style>
<p>
    @*@Html.ActionLink("Create New", "Create")*@
</p>

<div class="col-lg-12 linkone">
    <div class="card">
        <div class="card-body">
            <div class="table-responsive">
                <table class="table full-color-table full-inverse-table hover-table">
                    <thead>
                        <tr>
                            <th>
                                <label>Company </label>
                            </th>
                            <th>
                                <label>Trading Partner </label>
                            </th>
                            <th>
                                <label>Document Type </label>
                            </th>
                            <th>
                                <label>Document Number </label>
                            </th>
                            <th>
                                <label>Alt Document </label>
                            </th>
                            <th>
                                <label>Stor Number </label>
                            </th>
                            <th>
                                <label>Amount </label>
                            </th>
                            <th>
                                <label>Date Recieved </label>
                            </th>
                            <th>
                                <label>Date Acknowledgement</label>
                            </th>
                        </tr>
                    </thead>
                    <tbody>

                        @foreach (var item in Model)
                        {
                            <tr>

                                <td style="display:none">
                                    @Html.DisplayFor(id => item.HeaderKey)
                                </td>
                                <td>
                                    Company
                                </td>
                                <td>
                                    Trading Partner
                                </td>
                                <td>
                                    Document Type
                                </td>
                                <td>
                                    Document Number
                                </td>
                                <td>
                                    Alt Document
                                </td>
                                <td>
                                    Stor Number
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.CUR02_CurrencyCode)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DTM02_RequestedPickupDate)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.BEG05_PODate)
                                </td>
                             
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

